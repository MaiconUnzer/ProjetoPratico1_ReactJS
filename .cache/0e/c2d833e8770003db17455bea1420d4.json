ds React.Component {\r\n  constructor(props) {\r\n    super(props);\r\n    this.state = { turma: {}, alunos: [] };\r\n    this.loadAlunos = this.loadAlunos.bind(this);\r\n  }\r\n\r\n  componentWillMount() {\r\n    this.setState(() => {\r\n      const { match } = this.props;\r\n      const turma = TurmaService.getTurma(match.params.id);\r\n      return {\r\n        turma\r\n      };\r\n    });\r\n    this.loadAlunos();\r\n  }\r\n\r\n  loadAlunos() {\r\n    this.setState(prevState => {\r\n      let alunos = AlunoService.getAlunos();\r\n      alunos = alunos.filter(\r\n        aluno =>\r\n          aluno.turmaId === undefined || aluno.turmaId === prevState.turma.id\r\n      );\r\n\r\n      return {\r\n        alunos\r\n      };\r\n    });\r\n  }\r\n\r\n  render() {\r\n    const { turma, alunos } = this.state;\r\n    return (\r\n      <React.Fragment>\r\n        <div className=\"main-option-title\">{turma.name}</div>\r\n        <div className=\"row\">\r\n          <div className=\"col-md-12\">\r\n            <ul className=\"list-group\">\r\n              {alunos.map(aluno => {\r\n                return (\r\n                  <li className=\"list-group-item\" key={aluno.id}>\r\n                    <span>{aluno.name}</span>\r\n                    <span>\r\n                      {aluno.turmaId === turma.id ? (\r\n                        <React.Fragment>\r\n                          <span>\r\n                            <i className=\"material-icons\">check</i>\r\n                          </span>\r\n                          <button\r\n                            onClick={() => {\r\n                              AlunoService.removeAluno(turma.id, aluno.id);\r\n                              this.loadAlunos();\r\n                            }}\r\n                          >\r\n                            Excluir Aluno\r\n                          </button>\r\n                        </React.Fragment>\r\n                      ) : (\r\n                        <React.Fragment>\r\n                          <span>\r\n                            <i className=\"material-icons\">close</i>\r\n                          </span>\r\n\r\n                          <button\r\n                            className=\"btn-add-primary\"\r\n                            onClick={() => {\r\n                              AlunoService.addAluno(turma.id, aluno.id);\r\n                              this.loadAlunos();\r\n                            }}\r\n                          >\r\n                            Incluir Aluno\r\n                          </button>\r\n                        </React.Fragment>\r\n                      )}\r\n                    </span>\r\n                  </li>\r\n                );\r\n              })}\r\n            </ul>\r\n          </div>\r\n        </div>\r\n      </React.Fragment>\r\n    );\r\n  }\r\n}\r\n\r\nexport default AlunosXTurma;\r\n"},"lineCount":null}},"error":null,"hash":"c0aa9d76be836c463f2b856267b72b12","cacheData":{"env":{}}}